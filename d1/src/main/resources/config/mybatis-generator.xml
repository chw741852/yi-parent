<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <context id="context1">
        <plugin type="com.yi.mybatis.plugin.OverIsMergeablePlugin"/>
        <commentGenerator type="com.yi.mybatis.plugin.MyCommentGenerator"/>

        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/db_test?useUnicode=true&amp;characterEncoding=UTF-8&amp;useSSL=false&amp;serverTimezone=Asia/Shanghai"
                        userId="root" password="123123"/>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!-- 生成模型的包名和位置 此处要用绝对路径-->
        <javaModelGenerator targetPackage="com.yi.d1.domain" targetProject="src/main/java"/>
        <!-- 生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="com.yi.d1.mapper" targetProject="src/main/resources"/>
        <!-- 生成DAO的包名和位置-->
        <!--<javaClientGenerator targetPackage="com.yi.d1.mapper" targetProject="src/main/java" type="XMLMAPPER"/>-->

        <!-- 要生成哪些表-->
        <table tableName="test_user" domainObjectName="User" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"/>

    </context>
</generatorConfiguration>